  var i = 0

  while (i < cards.length) {
    if(cards[i].dataset.engagementId > cards[i+1].dataset.engagementId) {
        first_sort.push(cards[i+ 1])
    }
    else {
      first_sort.push(cards[i])
    }
    i += 1
  }

